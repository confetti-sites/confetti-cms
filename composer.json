{
  "require": {
    "eftec/bladeone": "^4.16",
    "confetti-cms/foundation": "*",
    "confetti-cms/bool": "*",
    "confetti-cms/image": "*",
    "confetti-cms/content": "*",
    "confetti-cms/discussion": "*",
    "confetti-cms/form": "*",
    "confetti-cms/select-file": "*",
    "confetti-cms/select": "*",
    "confetti-cms/list": "*",
    "confetti-cms/hidden": "*",
    "confetti-cms/text": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "pkg/*/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "ConfettiCms\\Parser\\": ".confetti/parser",
      "model\\": ".confetti/db/model",
      "query\\": ".confetti/db/query",
      "App\\": "/src/App/"
    },
    "files": [
      ".confetti/db/model.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
